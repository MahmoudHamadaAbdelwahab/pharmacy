@* create inject to class it's containe general data , it's use system asp.net endbentsy enjuction  & and use in website ) *@

@inject SignInManager<ApplicationUser> _sinInManager;
<!DOCTYPE html>
<html lang="en">

<head>
    <title>Pharma &mdash; Colorlib Template</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Rubik:400,700|Crimson+Text:400,400i" rel="stylesheet">
    @* bootstrap 5  *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    @* font-awesome *@
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" rel="stylesheet" />
    
    
    <link rel="stylesheet" href="~/pharmacy/fonts/icomoon/style.css">
    <link rel="stylesheet" href="~/pharmacy/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/pharmacy/css/magnific-popup.css">
    <link rel="stylesheet" href="~/pharmacy/css/jquery-ui.css">
    <link rel="stylesheet" href="~/pharmacy/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/pharmacy/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/pharmacy/css/aos.css">
    <link rel="stylesheet" href="~/pharmacy/css/style.css">

    @RenderSection("Styles", required: false);
</head>

<body>

    <div class="site-wrap">

        <div class="site-navbar py-2">

            <div class="search-wrap">
                <div class="container">
                    <a href="#" class="search-close js-search-close"><span class="icon-close2"></span></a>
                    <form action="#" method="post">
                        <input type="text" class="form-control" placeholder="Search keyword and hit enter...">
                    </form>
                </div>
            </div>

            <div class="container">
                <div class="d-flex align-items-center justify-content-between">
                    <div class="logo">
                        <div class="site-logo">
                            <a href="index.html" class="js-logo-clone">Pharma</a>
                        </div>
                    </div>
                    <div class="main-nav d-none d-lg-block">
                        <nav class="site-navigation text-right text-md-center" role="navigation">
                            <ul class="site-menu js-clone-nav d-none d-lg-block">
                                <li class="active"><a href="/Home/Index">Home</a></li>
                                <li><a href="/Admin/Home/Index">Admin</a></li>
                                <li><a href="/Store/Index">Store</a></li>

                                <li class="has-children">
                                    <a href="#">Drug classifications</a>
                                    <ul class="dropdown">
                                        @if (ViewBag.Classifications != null)
                                        {
                                            foreach (var classification in ViewBag.Classifications)
                                            {
                                                    <li><a href="#">@classification.ProdcutsClassificationName</a></li>
                                            }
                                        }
                                    </ul>
                                </li>
                                <li><a href="/Products/List">Products</a></li>
                                 @* <li><a href="/About/Index">About</a></li>  *@
                                <li><a href="/ContactUs/Index">Contact</a></li>
                            </ul>
                        </nav>
                    </div>
                    <div class="icons">
                        <a href="#" class="icons-btn d-inline-block js-search-open"><span class="icon-search"></span></a>
                        <a href="/Order/Cart" class="icons-btn d-inline-block bag">
                            <span class="icon-shopping-bag"></span>
                            <span class="number">2</span>
                        </a>
                        <a href="#" class="site-menu-toggle js-menu-toggle ml-3 d-inline-block d-lg-none">
                            <span class="icon-menu"></span>
                        </a>
                    </div>
                    <div class="dropdown">
                        <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            My Account
                        </button>
                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                              @if (_sinInManager.IsSignedIn(User))
                            {
                            <li><a asp-controller="Order" asp-action="MyOrders" data-lng="es">My Orders</a></li>
                            <li><a asp-controller="Users" asp-action="LoginOut" data-lng="es">Logout</a></li>
                            }
                            else
                            {
                            <li><a asp-controller="Users" asp-action="Login" data-lng="en">Login</a></li>
                            } 
                        </div>
                    </div>
                </div>
            </div>
        </div>

        @RenderBody()

        <footer class="site-footer">
            <div class="container">
                <div class="row">
                    <div class="col-md-6 col-lg-3 mb-4 mb-lg-0">

                        <div class="block-7">
                            <h3 class="footer-heading mb-4">About Us</h3>
                            <p id="About">
                              @*   Lorem ipsum dolor sit amet, consectetur adipisicing elit. Eius quae reiciendis distinctio voluptates
                                sed dolorum excepturi iure eaque, aut unde. *@
                            </p>
                        </div>

                    </div>
                    <div class="col-lg-3 mx-auto mb-5 mb-lg-0">
                        <h3 class="footer-heading mb-4">Quick Links</h3>
                        <ul class="list-unstyled">
                            <li><a href="#">Supplements</a></li>
                            <li><a href="#">Vitamins</a></li>
                            <li><a href="#">Diet &amp; Nutrition</a></li>
                            <li><a href="#">Tea &amp; Coffee</a></li>
                        </ul>
                    </div>

                    <div class="col-md-6 col-lg-3">
                        <div class="block-5 mb-5">
                            <h3 class="footer-heading mb-4">Contact Info</h3>
                            <ul class="list-unstyled">
                                <li class="address">203 Fake St. Mountain View, San Francisco, California, USA</li>
                                <li class="phone"><a href="tel://23923929210">+2 392 3929 210</a></li>
                                <li class="email">emailaddress@domain.com</li>
                            </ul>
                        </div>
                       
                        <div class="d-flex justify-content-around">
                                <a id="likInstagram" style="cursor : pointer"> <i class="fa-brands fa-instagram" aria-hidden="true"></i> </a>
                            
                            <a id="likFacebook" href="#">
                                <i class="fa-brands fa-facebook" aria-hidden="true"></i>
                            </a>

                            <a id="likYoutube" href="#">
                                <i class="fa-brands fa-youtube" aria-hidden="true"></i>
                            </a>

                            <a id="likTwitter" href="#">
                                <i class="fa-brands fa-twitter" aria-hidden="true"></i>
                            </a>

                        </div>

                    </div>
                </div>
                <div class="row pt-5 mt-5 text-center">
                    <div class="col-md-12">
                        <p>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made
                            with <i class="icon-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank"
                                                                                     class="text-primary">Colorlib</a>
                            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
                        </p>
                    </div>

                </div>
            </div>
        </footer>
    </div>

    <script src="~/pharmacy/js/jquery-3.3.1.min.js"></script>
    <script src="~/pharmacy/js/jquery-ui.js"></script>
    <script src="~/pharmacy/js/popper.min.js"></script>
    <script src="~/pharmacy/js/bootstrap.min.js"></script>
    <script src="~/pharmacy/js/owl.carousel.min.js"></script>
    <script src="~/pharmacy/js/jquery.magnific-popup.min.js"></script>
    <script src="~/pharmacy/js/aos.js"></script>
    <script src="~/pharmacy/js/main.js"></script>

    <script src="~/Modules/Helper.js"></script>
    <script src="~/Modules/Settings.js"></script>
